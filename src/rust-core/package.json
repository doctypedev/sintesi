{
  "name": "@doctypedev/core",
  "version": "0.3.1",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/doctypedev/doctype"
  },
  "napi": {
    "name": "doctype-core",
    "triples": {
      "defaults": true,
      "additional": [
        "aarch64-apple-darwin",
        "x86_64-unknown-linux-musl"
      ]
    }
  },
  "scripts": {
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "artifacts": "napi artifacts"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0"
  },
  "engines": {
    "node": ">= 10"
  },
  "publishConfig": {
    "access": "public"
  },
  "optionalDependencies": {
    "@doctypedev/core-win32-x64-msvc": "0.2.21",
    "@doctypedev/core-darwin-x64": "0.2.21",
    "@doctypedev/core-linux-x64-gnu": "0.2.21",
    "@doctypedev/core-darwin-arm64": "0.2.21",
    "@doctypedev/core-linux-x64-musl": "0.2.21"
  }
}
