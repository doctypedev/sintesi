{
  "name": "@doctypedev/doctype",
  "version": "0.4.5",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/doctypedev/doctype.git"
  },
  "description": "The Self-Maintaining Documentation System - Automatically update documentation when code changes",
  "main": "dist/cli/src/index.js",
  "types": "dist/cli/src/index.d.ts",
  "bin": {
    "doctype": "dist/cli/src/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "postbuild": "chmod +x dist/cli/src/index.js",
    "dev": "tsc --watch -p tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "documentation",
    "ast",
    "typescript",
    "automation",
    "drift-detection"
  ],
  "author": "doctypedev",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.53",
    "@ai-sdk/google": "^2.0.44",
