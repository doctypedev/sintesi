    "@ai-sdk/openai": "^2.0.77",
    "@ai-sdk/provider": "^2.0.0",
    "@clack/prompts": "^0.11.0",
    "@doctypedev/core": "workspace:*",
    "@types/uuid": "^10.0.0",
    "ai": "^5.0.106",
    "dotenv": "^17.2.3",
    "ts-morph": "^21.0.1",
    "uuid": "^13.0.0",
    "yargs": "^17.7.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/vue": "^1.0.31",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "eslint": "^8.56.0",
    "glob": "^13.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitepress": "^1.6.4",
    "vitest": "^1.1.0",
    "vue": "^3.5.25"
  },
  "engines": {
    "node": ">=20.0.0"
  },
